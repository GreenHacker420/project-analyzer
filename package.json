{
  "name": "projectify-cli",
  "version": "2.1.0",
  "description": "Project Analyzer using LangChain",
  "main": "dist/index.js",
  "bin": {
    "projectify": "dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GreenHacker420/project-analyzer.git"
  },
  "files": [
    "dist",
    "package.json",
    "README.md",
    "graph.png",
    "functions.png"
  ],
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc && cp src/report/template.html dist/report/ && echo '#!/usr/bin/env node' | cat - dist/index.js > dist/index.js.tmp && mv dist/index.js.tmp dist/index.js && chmod +x dist/index.js",
    "start": "node dist/index.js",
    "test": "jest",
    "dev": "ts-node src/index.ts"
  },
  "keywords": [
    "analysis",
    "langchain",
    "code-analysis"
  ],
  "author": "GreenHacker",
  "license": "ISC",
  "dependencies": {
    "@babel/parser": "^7.24.4",
    "@babel/traverse": "^7.24.1",
    "@google/generative-ai": "^0.24.1",
    "@langchain/core": "^1.1.8",
    "@langchain/google-genai": "^2.1.3",
    "@langchain/langgraph": "^1.0.7",
    "@langchain/ollama": "^1.1.0",
    "@langchain/openai": "^1.2.0",
    "@types/inquirer": "^8.2.12",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^17.2.3",
    "fast-glob": "^3.3.2",
    "fs-extra": "^11.2.0",
    "graphology": "^0.25.4",
    "inquirer": "^8.2.7",
    "langchain": "^1.2.3",
    "ora": "^5.4.1",
    "simple-git": "^3.30.0",
    "type-fest": "^5.3.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  }
}
